{
  "name": "Link Extractor",
  "version": "1.0",
  "description": "Get all the links from a website",
  "manifest_version": 2,
  "content_scripts": [
      {
          "matches": ["http://127.0.0.1:5500/testing-form.html"],
          "js": ["content.js"],
          "all_frames": false
      }
     
  ],
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/@google/generative-ai/+esm https://esm.run/@google/generative-ai;",

  "browser_action": {
      "default_popup": "popup.html"
  },
  "permissions": [
      "activeTab",
      "contextMenus",
      "declarativeContent", 
      "storage",
      "tabs"
  ]
  
}